{
    "manifest_version": 2,

    "name": "Remove Tracking",
    "description": "Attempts to Remove Tracking.",
    "version": "1.0",

    "content_scripts": [ {
        "all_frames": true,
        "js": [ "jquery.min.js","content.js" ],
        "matches": [ "http://*/*", "https://*/*" ]
    }],
    "background_page": "background.html",
    "permissions": [
        "tabs", "http://*/*"
    ],
    "background": {
        "scripts": ["content.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}